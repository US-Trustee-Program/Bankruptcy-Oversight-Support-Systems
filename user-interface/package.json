{
  "name": "cams",
  "version": "0.1.0",
  "private": true,
  "license": "CC0 v1.0",
  "description": "Bankruptcy Oversight Support System - Graphical Frontend User Interface",
  "dependencies": {
    "@microsoft/applicationinsights-react-js": "^17.0.0",
    "@microsoft/applicationinsights-web": "^2.8.15",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@uswds/uswds": "^3.5.0",
    "dotenv": "^16.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "redux": "^4.2.1",
    "typescript": "^4.9.5",
    "vite-plugin-env-compatible": "^1.1.1",
    "web-vitals": "^3.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.34.1",
    "c8": "^8.0.1",
    "dependency-cruiser": "^13.1.0",
    "esbuild": "0.18.17",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "fetch-mock": "^9.11.0",
    "jsdom": "^22.1.0",
    "pa11y-ci": "^3.0.1",
    "prettier": "^3.0.0",
    "sass": "^1.58.3",
    "vite": "^4.4.7",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  },
  "eslintConfig": {
    "extends": []
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "nth-check": "2.0.1",
    "webpack": "^5.78.0",
    "semver": "7.5.2",
    "word-wrap": "@aashutoshrathi/word-wrap"
  },
  "scripts": {
    "start": "vite",
    "start:pa11y": "export CAMS_PA11Y=true && tsc && vite build && vite preview --port 3000",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "export CAMS_PA11Y=true && vitest run",
    "test:watch": "export CAMS_PA11Y=true && vitest watch",
    "coverage": "export CAMS_PA11Y=true && vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "pa11y:ci": "pa11y-ci",
    "preinstall": "npx npm-force-resolutions",
    "dependency-cruiser-graphical": "current_time=$(date +'%Y-%m-%d_%H%M') && npx depcruise --output-type dot --do-not-follow '^node_modules($|/)' public src | dot -T svg > ../architecture/dependency-cruiser/user-interface/dependency-graph_${current_time}.svg",
    "dependency-cruiser-ci": "npx depcruise --output-type err-long public src"
  }
}
