    "@types/node": "^16.18.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "dotenv": "^16.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.2",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/chai": "^4.3.4",
    "@types/jsdom-global": "^3.0.4",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "chai": "^4.3.7",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^21.1.1",
    "jsdom-global": "^3.0.2",
