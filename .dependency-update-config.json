{
  "minPackageAgeDays": 15,
  "allowedPackages": [
    "eslint",
    "prettier",
    "@typescript-eslint/eslint-plugin",
    "@typescript-eslint/parser"
  ],
  "projects": [
    "root",
    "backend",
    "common",
    "dev-tools",
    "test/e2e",
    "user-interface"
  ],
  "constraints": {
    "pinned": [],
    "majorVersionLock": {
      "eslint": "9",
      "mssql": "10"
    },
    "majorVersionDelay": {
      "*": 30
    }
  }
}
