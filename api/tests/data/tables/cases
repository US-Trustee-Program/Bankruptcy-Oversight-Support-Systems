CREATE TABLE Chapters (
    ChapterId int NOT NULL AUTO_INCRIMENT,
    Title varchar(10),
    PRIMARY KEY (ChapterId)
)

INSERT INTO Chapters (ChapterId, Title)
VALUES
  (7, 'Chapter 7'),
  (9, 'Chapter 9'),
  (11, 'Chapter 11'),
  (15, 'Chapter 17'),
  (19, 'Chapter 19');

CREATE TABLE Cases (
    CaseId int NOT NULL AUTO_INCREMENT,
    Chapter int NOT NULL DEFAULT 11,
    Analyst varchar(32),
    constraint fk_chapter FOREIGN KEY (Chapter) REFERENCES Chapters (ChapterId),
    PRIMARY KEY (Personid)
);
