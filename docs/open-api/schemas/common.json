{
  "CaseAssignment": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "pattern": "^[\\dA-Z]{3}-\\d{2}-\\d{5}$",
        "description": "Division code, year, sequence number."
      },
      "caseId": {
        "type": "string"
      },
      "userId": {
        "type": "string"
      },
      "role": {
        "type": "string"
      },
      "createdAt": {
        "type": "string",
        "format": "date-time"
      },
      "updatedAt": {
        "type": "string",
        "format": "date-time"
      }
    }
  },
  "CamsUserReference": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      "email": {
        "type": "string"
      }
    }
  }
}
